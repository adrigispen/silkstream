    "@aws-sdk/credential-providers": "^3.721.0",
    "@aws-sdk/lib-dynamodb": "^3.721.0",
    "@aws-sdk/s3-request-presigner": "^3.722.0",
    "@types/node": "^22.10.5",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
